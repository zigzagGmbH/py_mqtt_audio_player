#!/bin/sh

APPDIR=$(readlink -f "$0")
APPDIR=$(dirname "$APPDIR")
java -Xms64m -Xmx512m -Djna.nosys=true -Djava.library.path="$APPDIR:$APPDIR/lib" -cp "$APPDIR:$APPDIR/lib/dummy_mqtt_audio_player_controller.jar:$APPDIR/lib/jogl-all-2.5.0-natives-linux-armv6hf.jar:$APPDIR/lib/gluegen-rt-2.5.0-natives-linux-aarch64.jar:$APPDIR/lib/jogl-all-2.5.0-natives-linux-amd64.jar:$APPDIR/lib/core-4.4.4.jar:$APPDIR/lib/gluegen-rt-2.5.0.jar:$APPDIR/lib/jogl-all-2.5.0.jar:$APPDIR/lib/kotlin-stdlib-2.0.20.jar:$APPDIR/lib/gluegen-rt-2.5.0-natives-android-aarch64.jar:$APPDIR/lib/gluegen-rt-2.5.0-natives-linux-armv6hf.jar:$APPDIR/lib/gluegen-rt-main-2.5.0.jar:$APPDIR/lib/annotations-13.0.jar:$APPDIR/lib/jogl-all-2.5.0-natives-macosx-universal.jar:$APPDIR/lib/jogl-all-2.5.0-natives-linux-aarch64.jar:$APPDIR/lib/gluegen-rt-2.5.0-natives-windows-amd64.jar:$APPDIR/lib/gluegen-rt-2.5.0-natives-linux-amd64.jar:$APPDIR/lib/jogl-all-main-2.5.0.jar:$APPDIR/lib/jogl-all-2.5.0-natives-android-aarch64.jar:$APPDIR/lib/gluegen-rt-2.5.0-natives-macosx-universal.jar:$APPDIR/lib/jogl-all-2.5.0-natives-windows-amd64.jar:$APPDIR/lib/antlr-2.7.7.jar:$APPDIR/lib/org.eclipse.paho.client.mqttv3-1.2.0.jar:$APPDIR/lib/mqtt.jar:$APPDIR/lib/controlP5.jar" dummy_mqtt_audio_player_controller "$@"
